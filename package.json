{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tsc --noEmit --skipLibCheck && eslint 'src/*/**.{js,ts,tsx}' --quiet --fix",
    "lint:md": "remark ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "gatsby": "^2.18.6",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-netlify-cms": "^4.1.31",
    "gatsby-plugin-offline": "^3.0.26",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-remark-autolink-headers": "^2.1.19",
    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.6",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.9.7",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.2",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-protected-mailto": "^1.0.3",
    "snarkdown": "^1.2.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-plugin-styled-components": "^3.1.14",
    "gatsby-plugin-stylelint": "^3.2.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "stylelint-webpack-plugin": "^1.1.1",
    "typescript": "^3.7.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "no-descending-specificity": null
    }
  }
}
