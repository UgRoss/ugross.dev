    "next-pwa": "^5.6.0",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-use": "^17.4.0",
    "rehype-external-links": "^2.0.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
