{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tsc --noEmit && eslint 'src/*/**.{js,ts,tsx}' --quiet --fix",
    "lint:md": "remark ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "gatsby": "^2.9.4",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-remark-autolink-headers": "^2.0.18",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-remark": "^2.4.0",
    "gatsby-transformer-sharp": "^2.1.21",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-protected-mailto": "^1.0.3",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-plugin-styled-components": "^1.10.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-stylelint": "^3.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.5.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "no-descending-specificity": null
    }
  }
}
