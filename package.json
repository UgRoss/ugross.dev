{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tsc --noEmit --skipLibCheck && eslint 'src/**/*.{js,ts,tsx}'",
    "lint:css": "stylelint 'src/*/**.{css,scss}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --check --pattern 'src/**/*.{js,jsx,tsx,ts,scss,css}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**.{js,jsx,ts,tsx}": "eslint --cache",
    "src/**.{css,scss}": "stylelint --config .stylelintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.28.0",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-feed": "^2.9.0",
    "gatsby-plugin-google-analytics": "^2.7.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-mdx": "^1.6.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.10.0",
    "gatsby-plugin-typescript": "^2.8.0",
    "gatsby-remark-copy-linked-files": "^2.6.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.7.0",
    "gatsby-remark-prismjs": "^3.9.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^2.7.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.21.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-use": "^15.3.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.14.10",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-plugin-stylelint": "^3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "^4.1.2"
  }
}
