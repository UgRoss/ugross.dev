{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:css",
    "lint:ts": "tsc --noEmit --skipLibCheck && eslint 'src/**.{js,ts,tsx}' --quiet --fix",
    "lint:md": "remark .",
    "lint:css": "stylelint 'src/*/**.{css,scss}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --check 'src/**.{js,jsx,tsx,ts,scss,css}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**.{js,jsx,ts,tsx}": "eslint --cache",
    "src/**.{css,scss}": "stylelint --config .stylelintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "@graphql-codegen/typescript-graphql-files-modules": "^1.13.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.20.10",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-graphql-codegen": "^2.7.0",
    "gatsby-plugin-graphql-loader": "^1.0.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-netlify-cms": "^4.2.2",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "graphql-tag": "^2.10.3",
    "hamburger-react": "^1.4.0",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.12.2",
    "normalize.css": "^8.0.1",
    "polished": "^3.5.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-protected-mailto": "^1.0.3",
    "react-use": "^13.27.1",
    "snarkdown": "^1.2.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-stylelint": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-scss": "^3.16.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "^3.8.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
