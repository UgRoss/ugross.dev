{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "2.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "npm run lint && gatsby build",
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tsc --noEmit --skipLibCheck && eslint 'src/**/*.{js,ts,tsx}'",
    "lint:css": "stylelint 'src/*/**.{css,scss}'",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "postcodegen": "prettier --write src/types/graphql.ts",
    "husky:precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**.{js,jsx,tsx,ts,scss,css}": "prettier --write",
    "src/**.{js,jsx,ts,tsx}": "eslint",
    "src/**.{css,scss}": "stylelint --config .stylelintrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tailwindcss/typography": "^0.5.2",
    "classnames": "^2.3.1",
    "gatsby": "^4.9.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-feed": "^4.9.0",
    "gatsby-plugin-google-analytics": "^4.9.0",
    "gatsby-plugin-image": "^2.9.0",
    "gatsby-plugin-manifest": "^4.9.0",
    "gatsby-plugin-mdx": "^3.9.0",
    "gatsby-plugin-offline": "^5.9.0",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-sass": "^5.9.0",
    "gatsby-plugin-sharp": "^4.9.0",
    "gatsby-plugin-typescript": "^4.9.0",
    "gatsby-remark-autolink-headers": "^5.9.0",
    "gatsby-remark-copy-linked-files": "^5.9.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.9.0",
    "gatsby-remark-prismjs": "^6.9.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^5.9.0",
    "gatsby-remark-table-of-contents": "^2.0.0",
    "gatsby-source-filesystem": "^4.9.0",
    "gatsby-transformer-sharp": "^4.9.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prism-react-renderer": "^1.3.1",
    "prismjs": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-use": "^17.3.2",
    "typeface-inter": "^3.18.1",
    "typeface-jetbrains-mono": "^2.0.1",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-postcss": "^5.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
}
